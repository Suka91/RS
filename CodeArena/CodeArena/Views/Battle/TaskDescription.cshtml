@model IEnumerable<CodeArena.Models.TaskDescription>

@{
    ViewBag.Title = "TaskDescription";
}

<h2>TaskDescription</h2>

<table>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.TaskTitle)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Level)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DescriptionText)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.TaskTitle)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Level)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DescriptionText)
            </td>
        </tr>
    }

</table>
